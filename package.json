    "jshint": "^2.8.0",
    "karma": "^6.2.0",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-requirejs": "^0.2.3",
    "mocha": "^8.3.2",
    "phantomjs-prebuilt": "^2.1.5"
